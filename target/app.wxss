.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.add{
  position: fixed;
  right: 40rpx;
  bottom: 60rpx;
  width: 100rpx;
  height: 100rpx;
  box-shadow: #666 0 4rpx 16rpx;
  background: #5295E3;
  text-align: center;
  border-radius: 50%;
  vertical-align: middle;
  z-index: 99;
}
.add-text{
  color: #fff;
  font-size: 80rpx;
  font-weight: 300;
  line-height: 90rpx;
}
.add-detail{
  display: block;
  box-sizing: border-box;
  position: fixed;
  z-index: 100;
  background: #fff;
  bottom: 0px;
  border-top: 1px solid #eee;
  box-shadow: #ccc 0px -1px 10px;
  width:100%;
  font-size: 28rpx;
  height: 100rpx;
}
.add-detail input{
  padding: 20rpx;
  padding-right: 100rpx;
}
.arrow-next{
  width: 60rpx;
  position: absolute;
  right: 10rpx;
  z-index: 10;
  bottom: 26rpx;
  transform: rotateY(180deg);
}
.list-box{
  height: 100vh;
  width: 100%;
  box-sizing: border-box;
}
.check-box{
  position: absolute;
  top: 16rpx;
}
.text-name{
  width: 560rpx;
  margin-left: 60rpx;
  white-space:nowrap;
  display: inline-block;
  overflow: hidden;
  text-overflow: ellipsis;
}